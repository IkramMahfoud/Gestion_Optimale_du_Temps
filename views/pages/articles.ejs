<!-- pages/articles.ejs -->

<!-- Assuming articles is an array of article objects -->
<ul>
    <% articles.forEach(function(article) { %>
      <li>
        <h2><%= article.title %></h2>
        <p><%= article.content %></p>
        <!-- Add more HTML to display other article properties as needed -->
      </li>
    <% }); %>
  </ul>
